{
  "_outboundAgent": {
    "{UUID}": "ws://localhost:8912/agents/connect"
  },
  "license": {
    "key": "{KEY}"
  },
  "http": {
    "host": "0.0.0.0",
    "port": 443,
    "certPath": "{FILE.PEM}",
    "keyPath": "{KEYFILE.PEM}"
  },
  "jwt": {
    "secret": "{SECRET}",
    "keystore": "{FILE.P12}",
    "type": "pkcs12",
    "pepper": "{PEPPER}"
  },
  "api.galley.run": {
    "openApiFile": "openapi.yaml",
    "cors": [
      "http://localhost:5173",
      "https://cloud.galley.dev",
      "https://cloud.galley.run"
    ],
    "host": "api.galley.dev"
  },
  "cloud.galley.run": {
    "host": "cloud.galley.dev",
    "cors": [
      "*"
    ]
  },
  "get.galley.run": {
    "protocol": "https://",
    "host": "get.galley.dev",
    "cors": [
      "*"
    ]
  },
  "db": {
    "port": 54332,
    "database": "galley",
    "host": "localhost",
    "pool_size": 5,
    "username": "postgres",
    "password": "postgres"
  },
  "messaging": {
    "email": {
      "hostname": "127.0.0.1",
      "port": 25,
      "username": "patrick@galley.run",
      "password": "{PASSWORD}",
      "defaults": {
        "from": "Galley <welcome@galley.run>",
        "urlPrefix": "http://localhost:5173"
      }
    }
  },
  "base_url": "https://api.galley.dev",
  "oauth": {
    "github": {
      "client_id": "{CLIENT ID}",
      "client_secret": "{CLIENT SECRET}",
      "app_id": null
    },
    "gitlab": {
      "client_id": "",
      "client_secret": "",
      "site": "https://gitlab.com"
    },
    "bitbucket": {
      "client_id": "",
      "client_secret": ""
    }
  }
}
